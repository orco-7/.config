<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'Ctrl+X' or 'Shift+Delete'</ShortCut>
          <Description>Cut</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="ProblemsView">
        <Item>
          <IdeaActionID>ActivateProblemsViewToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>ProblemsView</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reload All from Disk">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Y'</ShortCut>
          <Description>Reload All from Disk</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Save All">
        <Item>
          <IdeaActionID>SaveAll</IdeaActionID>
          <ShortCut>'Ctrl+S'</ShortCut>
          <Description>Save All</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Context Actions">
        <Item>
          <IdeaActionID>ShowIntentionActions</IdeaActionID>
          <ShortCut>'Alt+Enter'</ShortCut>
          <Description>Show Context Actions</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>